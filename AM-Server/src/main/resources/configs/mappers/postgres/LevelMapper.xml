<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.tscloud.address.server.mapper.LevelMapper">
    <sql id="LevelColumns">
    a.level as Level
    </sql>
    <select id="getLevelByID" resultType="com.tscloud.address.domain.entity.LevelEntity" parameterType="java.util.Map">
        SELECT <include refid="LevelColumns"/> FROM v_addresslevel a
        <where>
            <if test="addrId!=null">
                a.id=#{addrId}
            </if>
        </where>
    </select>
</mapper>
