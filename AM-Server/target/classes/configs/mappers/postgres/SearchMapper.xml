<mapper namespace="com.tscloud.address.server.mapper.SearchMapper">
    <sql id="SearchColums">
       a.id  as AddrId,
       a.address as Address,
       a.x as x,
       a.y as y,
       a.adcode as Adcode,
       a.poi_type as PoiType,
       a.name as Name
    </sql>
    <select id="SearchByID" resultType="com.tscloud.address.domain.entity.SearchEntity" parameterType="java.util.Map">
        SELECT <include refid="SearchColums"/>FROM v_village a
        <where>
            <if test="addrId!=null">
                a.id =#{addrId}
            </if>
        </where>
    </select>
